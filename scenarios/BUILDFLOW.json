        "id": "custom_python_2irEzGJljH3/QOL5k7/MeA",
        "type": "custom_python",
        "name": "ExtractMetrics",
        "runConditionType": "RUN_IF_STATUS_MATCH",
        "runConditionStatuses": [
          "SUCCESS",
          "WARNING"
